{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "OcppAdapter": {
    "ListenUrl": "http://*:5000",
    "BootNotificationTimeout": "00:01:00",
    "BusinessEventTimeout": "00:00:30",
    "CommandResponseTimeout": "00:01:00",
    "MessageSendTimeout": "00:00:10",
    "HeartbeatInterval": "00:05:00",
    "HeartbeatTimeoutMultiplier": 2.5,
    "EnableAuthentication": false,
    "MaxConcurrentConnections": 10000,
    "MessageQueueSize": 1000,
    "StateRetentionTime": "00:10:00",
    "EnableJsonSchemaValidation": true,
    "DuplicateConnectionStrategy": "Replace",
    "SessionInactivityTimeout": "00:30:00",
    "MessageReceiveTimeout": "00:05:00",
    "ConnectionTimeout": "00:00:30",
    "EnableHeartbeatMonitoring": true,
    "HeartbeatCheckInterval": "00:01:00",
    "SessionCleanupInterval": "00:01:00",
    "CallMatcherCleanupInterval": "00:00:30",
    "SupportedProtocols": [ "ocpp1.6", "ocpp1.5" ],
    "DefaultProtocol": "ocpp1.6"
  }
}
